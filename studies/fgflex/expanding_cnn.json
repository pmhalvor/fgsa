{
    "name": "fgflex-expanding-cnn",

    "model_name": "fgflex",
    
    "cnn_dim": 768,
    "expanding_cnn": [0, 2, 3, 4],
    "stack_count": 1,

    "dropout": 0.4,

    "loss_weight": 7, 
    "bert_finetune": true,
    "bert_expression": true,
    "bert_holder": true,
    "bert_polarity": true,
    "bert_target": true,
    "metric": "absa",

    "optimizer_name": "adam",
    "weight_decay": 0,

    "lr": 1e-5,

    "warm_up_constant": 5,
    "find_scope": false,

    "epochs": [150, 150, 150]
}