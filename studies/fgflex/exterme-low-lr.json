{
    "name": "fgflex-extreme-low-lr",

    "model_name": "fgflex",
    
    "cnn_dim": 768,
    "expanding_cnn": 0,
    "stack_count": 1,

    "dropout": 0.4,

    "loss_weight": 27, 
    "bert_finetune": true,
    "bert_polarity": true,
    "bert_target": true,
    "metric": "absa",

    "optimizer_name": "adam",
    "weight_decay": 0.5,

    "lr": 1e-9,
    "attn_lr": 1e-9,
    "expression_lr": 1e-9,
    "holder_lr": 1e-9,
    "polarity_lr": 1e-9,
    "target_lr": [1e-9, 1e-8, 1e-7],
    "shared_lr": 1e-9,

    "warm_up_constant": 5,
    "find_scope": false,

    "attention_relations": [
        [
            ["shared", "all"]
        ]
    ],
    "epochs": [10, 10, 10]
}
