{
    "name": "fgflex-imn",

    "model_name": "fgflex",

    "subtasks": ["expression, "polarity", "target"],    
    "attention_relations": [
        [
            ["polarity", "expression"]
        ]
    ],
    "split_cnn_kernels": [[3, 5]],

    "cnn_dim": 300,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "kernel_size": 5,
    "stack_count": 1,

    "dropout": 0.4,
    "loss_weight": 1, 
    "optimizer_name": "adam",
    "weight_decay": 0,
    
    "lr": 1e-5,
    
    "metric": "absa",

    "warm_up_constant": 3.5,
    "epochs": [30, 30, 30]
}
