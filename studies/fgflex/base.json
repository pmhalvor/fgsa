{
    "name": "fgflex-base",

    "model_name": "fgflex",
    
    "cnn_dim": 768,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "kernel_size": 5,
    "stack_count": 2,
    "split_cnn_kernels": [[3, 7, 11]],

    "dropout": 0.4,
    "optimizer_name": "adam",
    "weight_decay": 0,
    "lr": 1e-5,
    "warm_up_constant": 3.5,

    "metric": "absa",

    "epochs": [30, 30, 30]
}
