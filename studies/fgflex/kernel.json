{
    "name": "fgflex-kernel-size",

    "model_name": "fgflex",
    
    "kernel_size": [3, 5, 7, 9, 11, 13],
    "cnn_dim": 768,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "stack_count": 1,
    "attention_relations": [[["polarity", "expression"]]],
    "split_cnn_kernels":[[3, 7, 11]],

    "dropout": 0.4,
    "loss_weight": 1, 
    "optimizer_name": "adam",
    "weight_decay": 0,
    "warm_up_constant": 5,
    "use_cnn_dropout": true,
    "use_linear_dropout": true,
    
    "lr": 1e-5,
    
    "metric": "absa",

    "epochs": [25, 25, 25]
}
