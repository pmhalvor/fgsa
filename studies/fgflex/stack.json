{
    "name": "fgflex-stack",
    
    "model_name": "fgflex",
    
    "cnn_dim": 768,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "kernel_size": 5,
    "stack_count": [1, 2, 3, 4],
    "split_cnn_kernels": [[3, 7, 11]],
    
    "dropout": 0.4,

    "loss_weight": 1, 
    "bert_finetune": true,
    "optimizer_name": "adam",
    "weight_decay": 0,
    "use_linear_dropout": [true, false],
    "use_cnn_dropout": [false, true],
    
    "lr": 1e-5,
    
    "metric": "absa",

    "warm_up_constant": 3.5,
    "epochs": [30, 30, 30]
}
