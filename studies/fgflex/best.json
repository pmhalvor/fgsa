{
    "name": "fgflex-best",
    "empty": null,

    "model_name": "fgflex",
    
    "cnn_dim": 3072,
    "use_cnn_dropout": true,
    "use_linear_dropout": true,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "kernel_size": 7,
    "stack_count":2, 
    "shared_layers":3,
    "expression_layers": 2,
    "target_layers": 2,
    "attention_relations": [
        [
            ["polarity", "expression"],
            ["polarity", "target"]
        ]
    ],
    "split_cnn_kernels":[[3, 5, 7, 13, 3, 5, 7, 13]],
    "split_cnn_tasks": [
        ["expression", "target", "polarity"]
    ],

    "bert_finetune": [true, false],
    "dropout": 0.4,
    "loss_weight": 1, 
    "optimizer_name": "adam",
    "weight_decay": 0,
    "warm_up_constant": 5,
    
    "lr": 1e-5,
    
    "metric": "absa",

    "epochs": 200
}