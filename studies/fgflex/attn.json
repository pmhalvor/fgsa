{

    "name": "fgflex-attn",
    
    "model_name": "fgflex",
    
    "cnn_dim": 768,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "kernel_size": 5,
        "attention_relations": [
        [
            ["target", "target"],
            ["expression", "expression"],
            ["holder", "holder"],
            ["polarity", "polarity"]
        ],
        [
            ["target", "expression"],
            ["expression", "target"],
            ["target", "polarity"],
            ["expression", "polarity"]
        ],
        [
            ["expression", "polarity"]
        ]
    ],

    "stack_count": 1,

    "dropout": 0.4,

    "loss_weight": 1, 
    "bert_finetune": true,
    "optimizer_name": "adam",
    "weight_decay": 0,
    "warm_up_constant": 5,
    "use_linear_dropout": [true, false],
    "use_cnn_dropout": [true, false],
    
    "lr": 1e-5,
    
    "metric": "absa",

    "epochs": [25, 25, 25]
}