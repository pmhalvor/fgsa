{

    "name": "fgflex-attn",
    
    "model_name": "fgflex",
    
    "cnn_dim": 768,
    "expanding_cnn": 0,
    "gold_transmission": true,
    "kernel_size": 5,
    "attention_relations": [
        [
            ["target", "target"],
            ["expression", "expression"]
        ],
        [
            ["target", "expression"],
            ["expression", "target"],
            ["polarity", "expression"]
        ],
        [
            ["target", "target"],
            ["expression", "expression"],
            ["holder", "holder"],
            ["polarity", "polarity"]
        ],
        [
            ["target", "expression"],
            ["expression", "target"],
            ["polarity", "target"],
            ["polarity", "expression"]
        ]

    ],
    "split_cnn_kernels": [[3, 5]],

    "stack_count": 1,

    "dropout": 0.4,

    "loss_weight": 1, 
    "bert_finetune": true,
    "optimizer_name": "adam",
    "weight_decay": 0,
    "warm_up_constant": 5,
    "use_linear_dropout": true,
    "use_cnn_dropout": false,
    
    "lr": 1e-5,
    
    "metric": "absa",

    "epochs": [50, 50, 50]
}
