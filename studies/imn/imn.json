{
    "name": "weights-frostbert",

    "model_name": "imn",

    "dropout": 0.4,

    "loss_weight": [33, 37], 
    "bert_finetune": false,
    "metric": "hard",

    "optimizer_name": "adam",
    "weight_decay": [0.5, 0.7],

    "lr": 1e-9,
    "expression_lr": 1e-7,
    "polarity_lr": 1e-7,
    "target_lr": 1e-7,

    "cnn_dim": 768,
    "shared_layers": 5,
    "target_layers": 2,
    "polarity_layers": 2,
    "expression_layers": 2,
    "interactions": 3,    

    "warm_up_constant": 5,
    "find_scope": [true, false],

    "epochs": 100
}
