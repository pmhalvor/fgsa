{
    "name": "imn-interactions",
    "model_name": "imn",

    "find_scope": false,
    "cnn_dim": 768,
    "shared_layers": 1,
    "target_layers": 1,
    "polarity_layers": 1,
    "expression_layers": 1,
    "interactions": [1, 2, 3, 4, 5],
    "query": "expression",
    "key": "polarity",
    "value": "polarity",
    "gold_transmission":true,

    "dropout": 0.4,
    "loss_weight": 1, 
    "optimizer_name": "adam",
    "weight_decay": 0,
    "warm_up_constant": 5,

    "lr": 1e-5,
    "lr_scheduler_patience":25,
    
    "metric": "absa",

    "epochs": [50]
}
